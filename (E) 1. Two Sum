public class Solution {
    public int[] TwoSum(int[] a, int target) {
            int[] retArr = new int[2];
    for(int i = 0; i < a.Length-1; i++)
    {
        for(int j = 1; j < a.Length; j++)
        {
            if(a[i] + a[j] == target)
            {
                retArr[0] = i;
                retArr[1] = j;
            }
        }
    }
    return retArr;
    }
}